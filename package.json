{
  "name": "ai-monitoring-agent",
  "version": "1.0.0",
  "description": "Enterprise-grade AI tool usage monitoring and control system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js start",
    "start:daemon": "node dist/index.js start --daemon",
    "start:test": "node dist/index.js test",
    "status": "node dist/index.js status",
    "agent:start": "npm run build && npm run start",
    "agent:test": "npm run build && npm run start:test",
    "service:install": "node dist/service-installer.js install",
    "service:uninstall": "node dist/service-installer.js uninstall",
    "service:start": "node dist/service-installer.js start",
    "service:stop": "node dist/service-installer.js stop",
    "service:restart": "node dist/service-installer.js restart",
    "service:status": "node dist/service-installer.js status",
    "api:dev": "ts-node src/central-management/api/start-server.ts",
    "api:start": "npm run build && node dist/central-management/api/start-server.js"
  },
  "keywords": [
    "ai-monitoring",
    "security",
    "enterprise",
    "typescript"
  ],
  "author": "AI Monitoring Team",
  "license": "MIT",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "commander": "^14.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "node-cron": "^3.0.3",
    "node-windows": "^1.0.0-beta.8",
    "uuid": "^11.1.0"
  }
}
